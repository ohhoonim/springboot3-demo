<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dev.ohhoonim.demo.mapper.PostMapper">
	<select id="listPost" resultType="dev.ohhoonim.demo.model.Post">
    	select 
			post_id,
			title,
			contents,
			author,
			created_date_time	
		from post 
	</select>	
</mapper>